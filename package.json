{
	"name": "postcss-animations",
	"version": "6.1.1",
	"description": "PostCSS plugin that adds `@keyframes` from animate.css, tuesday.css, magic.css, mimic.css",
	"license": "MIT",
	"main": "lib/index.js",
	"files": [
		"lib"
	],
	"scripts": {
		"compile": "babel src --out-dir lib",
		"lint": "eslint ./src/*.js",
		"precompile": "npm run lint",
		"prepare": "npm run test",
		"pretest": "npm run compile",
		"test": "mocha --compilers js:babel-register ./test/test.mjs"
	},
	"dependencies": {
		"postcss-value-parser": "^3"
	},
	"peerDependencies": {
		"postcss": "6.x"
	},
	"keywords": [
		"animate.css",
		"animation",
		"css",
		"keyframes",
		"magic.css",
		"mimic.css",
		"postcss",
		"postcss-plugin",
		"tuesday.css"
	],
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-eslint": "^8.2.1",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.5.2",
		"babel-register": "^6.24.1",
		"chai": "4.1.2",
		"eslint": "4.19.0",
		"mocha": "5.1.1",
		"postcss": "^6.0.0",
		"postcss-animation.css-data": "^1.0.0",
		"postcss-magic.css-data": "^1.0.0",
		"postcss-mimic.css-data": "^1.0.0",
		"postcss-tuesday.css-data": "^1.0.0"
	},
	"repository": "https://github.com/retyui/postcss-animations",
	"author": "David Narbutovich <vamnemne@gmail.com>",
	"engines": {
		"node": ">= 4.2"
	}
}
