{
	"name": "postcss-animations",
	"version": "6.1.0",
	"description": "PostCSS plugin that adds `@keyframes` from animate.css, tuesday.css, magic.css, mimic.css",
	"license": "MIT",
	"main": "lib/index.js",
	"files": ["lib"],
	"scripts": {
		"lint": "eslint ./src/*.js",
		"precompile": "npm run lint",
		"compile": "babel src --out-dir lib",
		"pretest": "npm run compile",
		"test": "mocha --compilers js:babel-register ./test/test.mjs",
		"prepublish": "npm run test"
	},
	"dependencies": {
		"postcss": "^6",
		"postcss-value-parser": "^3"
	},
	"keywords": [
		"postcss",
		"css",
		"postcss-plugin",
		"animation",
		"keyframes",
		"animate.css",
		"tuesday.css",
		"magic.css",
		"mimic.css"
	],
	"devDependencies": {
		"babel-cli": "^6.24.1",
		"babel-eslint": "^7.2.3",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.5.2",
		"babel-register": "^6.24.1",
		"chai": "3.5.0",
		"eslint": "3.19.0",
		"mocha": "3.4.2",
		"postcss-animation.css-data": "^0.1.0",
		"postcss-magic.css-data": "^0.1.0",
		"postcss-mimic.css-data": "^0.0.2",
		"postcss-tuesday.css-data": "^0.1.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/retyui/postcss-animations.git"
	},
	"author": {
		"name": "David Narbutovich",
		"email": "vamnemne@gmail.com",
		"url": "https://github.com/retyui"
	},
	"bugs": {
		"url": "https://github.com/retyui/postcss-animations/issues"
	},
	"homepage": "https://github.com/retyui/postcss-animations"
}
